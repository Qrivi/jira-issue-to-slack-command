{
  "name": "jira-issue-to-slack-command",
  "version": "1.0.0",
  "description": "Fetches Jira issue details on command.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js --exec \"npm run lintfix && node\"",
    "lint": "standardx",
    "lintfix": "standardx --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Qrivi/jira-issue-to-slack-command.git"
  },
  "author": "Kristof Dewilde <kristof.dewilde@designisdead.com>",
  "license": "MIT",
  "dependencies": {
    "base-64": "^0.1.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.3",
    "sync-request": "^6.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1",
    "standardx": "^4.0.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "env": {
      "es6": true
    },
    "rules": {
      "brace-style": [
        "error",
        "1tbs"
      ]
    }
  }
}
